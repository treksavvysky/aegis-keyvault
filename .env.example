# Copy this file to .env and fill in the values
# NEVER commit .env to version control

# JWT signing key (any strong secret string)
AEGIS_SIGNING_KEY=change-me

# Admin token for privileged endpoints (any strong secret string)
AEGIS_ADMIN_TOKEN=change-me

# Fernet encryption key for secrets vault
# Generate with: python -c "from cryptography.fernet import Fernet; print(Fernet.generate_key().decode())"
AEGIS_ENCRYPTION_KEY=
